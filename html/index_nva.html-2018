	},function(){

		slidebox.find("li>div").bind("mouseleave",function(e){

			jQuery(this).children("a").removeClass("on");

		});

		slidebox.find("li>div").delegate("a","mouseenter",function(){

			var that = jQuery(this);

			that.removeClass("on");

			that.siblings().addClass("on");

		});

	});

});

</script> 
</body>

</html>