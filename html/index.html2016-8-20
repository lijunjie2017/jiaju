	<script type="text/javascript">
function b(){
	h = $(window).height();
	t = $(document).scrollTop();
	if(t > h){
		$('#gotop').show();
	}else{
		$('#gotop').hide();
	}
}

$(document).ready(function(e) {
	
	b();
	
	$('#gotop').click(function(){
		$(document).scrollTop(0);	
	});
	
	$('#backi_OK').hover(function(){
		$(this).attr('id','backi_OK_hover');
		$('#backi_OK').show();
	},function(){
		$(this).attr('id','backi_OK');
		
	})

});

$(window).scroll(function(e){
	b();		
});
</script>
